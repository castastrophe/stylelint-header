{
	"name": "stylelint-header",
	"description": "A stylelint plugin that checks for a file header.",
	"version": "2.0.0",
	"license": "Apache-2.0",
	"author": "Cassondra Roberts <castastrophe@users.noreply.github.com> (https://allons-y.llc)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/castastrophe/stylelint-header.git"
	},
	"bugs": {
		"url": "https://github.com/castastrophe/stylelint-header/issues"
	},
	"type": "module",
	"module": "index.js",
	"exports": "./index.js",
	"scripts": {
		"postinstall": "husky || true",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"coverage": "yarn test --coverage --coverageReporters=cobertura",
		"semantic-release": "semantic-release",
		"prepublishOnly": "yarn test",
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
		"watch": "yarn test --watch"
	},
	"funding": [
		{
			"type": "github",
			"url": " https://github.com/sponsors/castastrophe"
		}
	],
	"engines": {
		"node": ">=20.8.1"
	},
	"files": [
		"index.js",
		"!test/*"
	],
	"keywords": [
		"stylelint",
		"header",
		"stylelint-plugin",
		"css",
		"linter"
	],
	"peerDependencies": {
		"stylelint": "^15.0.0 || ^16.0.0"
	},
	"dependencies": {
		"string-similarity": "^4.0.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^11.0.1",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.3",
		"cross-env": "^7.0.3",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-preset-stylelint": "^7.1.1",
		"lint-staged": "^15.4.1",
		"pinst": "^3.0.0",
		"lodash": "^4.17.21",
		"prettier": "^3.4.2",
		"prettier-package-json": "^2.8.0",
		"semantic-release": "^24.2.1",
		"stylelint": "^16.13.2"
	},
	"jest": {
		"preset": "jest-preset-stylelint"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"lint-staged": {
		"*.js": [
			"prettier --write"
		],
		"package.json": [
			"prettier-package-json --write"
		]
	}
}
